<div class="container">
  <div class="row">
    <% @cars.each do |c| %>
      <div class="card" style="width: 18rem;">
        <img src="..." class="card-img-top" alt="...">
        <div class="card-body">
        <h5 class="card-title"><%= c.name %></h5>
        <p class="card-text"><%= c.brand %></p>
        <%= link_to 'View', c, class: 'btn btn-primary' %>
      </div>
    </div>
    <% end %>
  </div>
</div>
